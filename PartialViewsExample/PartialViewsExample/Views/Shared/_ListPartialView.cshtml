@* ViewBag and ViewData is the same thing *@
<div class="list-container">
    @{
        // ViewBag.ListTitle = "Updated";
    }
    @* <h3>@ViewBag.ListTitle</h3> *@
    <h3>@ViewData["ListTitle"]</h3>
    <ul class="list">
        @foreach(string item in ViewBag.ListItems)
        {
            <li>@item</li>
        }
    </ul>
</div>